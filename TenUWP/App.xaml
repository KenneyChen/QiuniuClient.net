<Application
    x:Class="TenUWP.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:TenUWP"
    RequestedTheme="Light">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Style/Style.xaml"/>
                <ResourceDictionary Source="Style/Core.xaml"/>
            </ResourceDictionary.MergedDictionaries>
            <local:NullableBooleanToBoolean x:Key="NullableBooleanToBooleanKey" />
            <local:Int32ToColor x:Key="Int32ToColorKey" />
            <local:Int32ToVisibility x:Key="Int32ToVisibilityKey" />
        </ResourceDictionary>
    </Application.Resources>
</Application>
